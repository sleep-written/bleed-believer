{
  "name": "@bleed-believer/state",
  "version": "0.0.0-alpha.0",
  "description": "Assign path alias using tsconfig.json file",
  "type": "module",
  "types": "./dist/index.d.ts",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "files": [
    "dist"
  ],
  "exports": {
    "./esm": {
      "import": "./dist/loader/index.mjs"
    },
    ".": {
      "import": "./dist/index.js"
    }
  },
  "scripts": {
    "test": "ava",
    "build:tsc": "tsc -p tsconfig.build.json && tsc-alias -p tsconfig.json",
    "build:swc": "node ../cli/dist/cli.js build --config ./tsconfig.build.json",
    "build": "node --run clean && node --run build:swc && node --run build:tsc",
    "clean": "rm -rf ./dist",
    "check": "tsc --noEmit"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sleep-written/bleed-believer.git"
  },
  "keywords": [
    "ts",
    "typescript",
    "tsconfig",
    "ts-config",
    "tsconfig.json",
    "path",
    "alias",
    "mapping"
  ],
  "publishConfig": {
    "access": "public"
  },
  "author": "sleep-written",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/sleep-written/bleed-believer/issues"
  },
  "homepage": "https://github.com/sleep-written/bleed-believer#readme",
  "peerDependencies": {
    "@angular/compiler": "^21.0.0",
    "@angular/forms": "^21.0.0"
  }
}
