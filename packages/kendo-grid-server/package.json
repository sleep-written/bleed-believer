{
  "name": "@bleed-believer/kendo-grid-server",
  "version": "2.0.1",
  "description": "Parses the querystrings built with @bleed-believer/kendo-grid-client fro TypeORM",
  "type": "module",
  "types": "./dist/index.d.ts",
  "module": "./dist/index.js",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.js"
    }
  },
  "scripts": {
    "test": "ava",
    "build:tsc": "tsc -p tsconfig.build.json && tsc-alias -p tsconfig.json",
    "build:swc": "node ../cli/dist/cli.js build --config ./tsconfig.build.json",
    "build": "node --run clean && node --run build:swc && node --run build:tsc",
    "clean": "rm -rf ./dist",
    "check": "tsc --noEmit"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sleep-written/bleed-believer.git"
  },
  "keywords": [
    "typeorm",
    "odata",
    "kendo-ui",
    "progress"
  ],
  "publishConfig": {
    "access": "public"
  },
  "author": "sleep-written",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/sleep-written/bleed-believer/issues"
  },
  "homepage": "https://github.com/sleep-written/bleed-believer#readme",
  "dependencies": {
    "@progress/kendo-data-query": "^1.7.1",
    "typeorm": "^0.3.22"
  },
  "devDependencies": {
    "sqlite3": "^5.1.7"
  }
}
