{
  "name": "@bleed-believer/kendo-grid-server",
  "version": "0.0.4",
  "description": "Parses the querystrings built with @bleed-believer/kendo-grid-client fro TypeORM",
  "type": "module",
  "types": "./dist/index.d.ts",
  "module": "./dist/index.js",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.js"
    }
  },
  "scripts": {
    "clean": "rm -rf ./dist",
    "build": "tsc --project ./tsconfig.build.json --sourceMap false",
    "watch": "tsc --project ./tsconfig.build.json --watch",
    "test": "ava"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sleep-written/bleed-believer.git"
  },
  "keywords": [
    "typeorm",
    "odata",
    "kendo-ui",
    "progress"
  ],
  "publishConfig": {
    "access": "public"
  },
