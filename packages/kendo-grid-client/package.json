{
  "name": "@bleed-believer/kendo-grid-client",
  "version": "1.0.0",
  "description": "Easy manner to build tables with kendo-grid, using OData protocol.",
  "type": "module",
  "types": "./dist/index.d.ts",
  "module": "./dist/index.js",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.js"
    }
  },
  "scripts": {
    "clean": "rm -rf ./dist",
    "build": "tsc --project ./tsconfig.build.json --sourceMap false",
    "watch": "tsc --project ./tsconfig.build.json --watch",
