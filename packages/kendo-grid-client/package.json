{
  "name": "@bleed-believer/kendo-grid-client",
  "version": "1.0.6",
  "description": "Easy manner to build tables with kendo-grid, using OData protocol.",
  "type": "module",
  "types": "./dist/index.d.ts",
  "module": "./dist/index.js",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.js"
    }
  },
  "scripts": {
    "test": "ava",
    "build:tsc": "tsc -p tsconfig.build.json && tsc-alias -p tsconfig.json",
    "build:swc": "node ../cli/dist/cli.js build --config ./tsconfig.build.json",
    "build": "node --run clean && node --run build:swc && node --run build:tsc",
    "clean": "rm -rf ./dist",
    "check": "tsc --noEmit"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sleep-written/bleed-believer.git"
  },
  "keywords": [
    "odata",
    "kendo-ui",
    "progress"
  ],
  "publishConfig": {
    "access": "public"
  },
  "author": "sleep-written",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/sleep-written/bleed-believer/issues"
  },
  "homepage": "https://github.com/sleep-written/bleed-believer#readme",
  "peerDependencies": {
    "@progress/kendo-angular-grid": "^20.0.0",
    "@progress/kendo-data-query": "^1.7.1"
  }
}
