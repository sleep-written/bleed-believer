{
  "name": "@bleed-believer/espresso",
  "version": "0.11.2",
  "description": "Create endpoints for express.js using classes and decorators.",
  "type": "module",
  "types": "./dist/index.d.ts",
  "module": "./dist/index.js",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.js"
    }
  },
  "scripts": {
    "test": "npm run clean && swc ./src -d ./dist --strip-leading-paths --config-file .test.swcrc && ava",
    "build": "npm run clean && swc ./src -d ./dist --strip-leading-paths & tsc",
    "clean": "rm -rf ./dist",
    "check": "tsc --noEmit"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sleep-written/bleed-believer.git"
  },
  "keywords": [
    "express",
    "expressjs",
    "express.js",
    "endpoint",
    "endpoints",
    "routing",
    "api",
    "rest",
    "controller"
  ],
  "publishConfig": {
    "access": "public"
  },
  "author": "sleep-written",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/sleep-written/bleed-believer/issues"
  },
  "homepage": "https://github.com/sleep-written/bleed-believer#readme",
  "dependencies": {
    "@bleed-believer/meta": "^0.11.2"
  },
  "peerDependencies": {
    "express": "^4.18.1"
  }
}
