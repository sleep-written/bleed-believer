{
  "name": "@bleed-believer/espresso",
  "version": "0.0.2-alpha.0",
  "description": "Create endpoints for express.js using classes and decorators.",
  "type": "module",
  "types": "./dist/index.d.ts",
  "module": "./dist/index.js",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.js"
    }
  },
  "scripts": {
    "test": "ava",
    "build:tsc": "tsc -p tsconfig.build.json && tsc-alias -p tsconfig.json",
    "build:swc": "node ../cli/dist/cli.js build --config ./tsconfig.build.json",
    "build": "node --run clean && node --run build:swc && node --run build:tsc",
    "clean": "rm -rf ./dist",
    "check": "tsc --noEmit"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sleep-written/bleed-believer.git"
  },
  "keywords": [
    "express",
    "expressjs",
    "express.js",
    "endpoint",
    "endpoints",
    "routing",
    "api",
    "rest",
    "controller"
  ],
  "publishConfig": {
    "access": "public"
  },
  "author": "sleep-written",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/sleep-written/bleed-believer/issues"
  },
  "homepage": "https://github.com/sleep-written/bleed-believer#readme",
  "dependencies": {
    "@bleed-believer/meta": "^0.0.0-alpha.0"
  },
  "peerDependencies": {
    "@types/express": "^5.0.0",
    "express": "^5.0.0"
  }
}
