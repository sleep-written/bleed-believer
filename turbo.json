{
    "$schema": "https://turborepo.com/schema.json",
    "tasks": {
        "@bleed-believer/cli#build": {
            "cache": true,
            "inputs": [ "src/**" ],
            "outputs": [ "dist/**" ]
        },
        "build": {
            "cache": true,
            "inputs": [ "src/**" ],
            "outputs": [ "dist/**" ],
            "dependsOn": [ "@bleed-believer/cli#build", "^build" ]
        },
        "test": {
            "cache": true,
            "inputs": [ "src/**" ],
            "outputs": [ "dist/**" ],
            "dependsOn": [ "@bleed-believer/cli#build" ]
        }
    }
}